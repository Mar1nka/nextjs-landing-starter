{
  "presets": [
    [
      "next/babel"
    ]
  ],
  "plugins": [
    [
      "babel-plugin-inline-react-svg",
      {
        "svgo": {
          "plugins": [
            {
              "cleanupIDs": false
            }
          ]
        }
      }
    ],
    [
      "babel-plugin-module-resolver",
      {
        "alias": {
          "~": "./src/client"
        }
      }
    ]
  ]
}
